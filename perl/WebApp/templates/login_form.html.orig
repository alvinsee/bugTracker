= 343 =   <tr>
= 344 =     <td align="right">User name:</td>
= 345 =     <td><input type="text" name="authen_username" size=20 /></td>
= 346 =   </tr>
= 347 =   <tr>
= 348 =     <td align="right">Password:</td>
= 349 =     <td><input type="password" name="authen_password" size=20 /></td>
= 350 =   </tr>
= 351 =   <tr>
= 352 =     <td> &nbsp; </td>
= 353 =     <td><input type="submit" name="submit" value="Submit" /></td>
= 354 =   </tr>
= 355 = </table>
= 356 = <input type="hidden" name="destination" value="<TMPL_VAR DESTINATION>" />
= 357 = </form>
